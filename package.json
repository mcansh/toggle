{
  "name": "@mcansh/feature-flags",
  "version": "1.0.0",
  "description": "a feature flag platform",
  "main": "remix.config.js",
  "scripts": {
    "postinstall": "tsc -b",
    "build:css": "postcss styles --base styles --dir app/ --env production",
    "prebuild": "npm run clean",
    "build": "npm run build:css && remix build && tsc -b",
    "dev": "concurrently \"postcss styles --base styles --dir app/ -w\" \"remix run\" \"tsc -b -w --preserveWatchOutput\" \"cross-env NODE_ENV=development nodemon -w app -w build -w server -e js,jsx,ts,tsx,md,mdx server-build/index.js\"",
    "start": "node server-build/index.js",
    "clean": "del .cache build server-build public/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcansh/remix-feature-flags.git"
  },
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcansh/remix-feature-flags/issues"
  },
  "homepage": "https://github.com/mcansh/remix-feature-flags#readme",
  "dependencies": {
    "@prisma/client": "2.14.0",
    "@remix-run/cli": "0.9.0-pre.2",
    "@remix-run/core": "0.9.0-pre.2",
    "@remix-run/data": "0.9.0-pre.2",
    "@remix-run/express": "0.9.0-pre.2",
    "@remix-run/react": "0.9.0-pre.2",
    "argon2": "0.27.1",
    "connect-redis": "5.0.0",
    "date-fns": "2.16.1",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "ioredis": "4.19.4",
    "ms": "2.1.3",
    "nodemailer": "6.4.17",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "slugify": "1.4.6",
    "type-fest": "0.20.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.14.0",
    "@tailwindcss/forms": "0.2.1",
    "@types/connect-redis": "0.0.16",
    "@types/dotenv-safe": "8.1.1",
    "@types/express": "4.17.9",
    "@types/express-session": "1.17.3",
    "@types/ms": "0.7.31",
    "@types/node": "14.14.20",
    "@types/nodemailer": "6.4.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "autoprefixer": "10.2.0",
    "concurrently": "5.3.0",
    "cross-env": "7.0.3",
    "del-cli": "3.0.1",
    "nodemon": "2.0.7",
    "postcss": "8.2.2",
    "postcss-cli": "8.3.1",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3"
  }
}
