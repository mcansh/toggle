{
  "name": "@mcansh/feature-flags",
  "version": "1.0.0",
  "description": "a feature flag platform",
  "main": "remix.config.js",
  "scripts": {
    "postinstall": "npm run build:icons && tsc -b",
    "build:css": "postcss styles --base styles --dir app/ --env production",
    "build:icons": "node scripts/convert-herocions.js",
    "prebuild": "npm run clean && npm run build:icons",
    "build": "npm run build:css && remix build && tsc -b",
    "dev": "pm2-dev pm2.config.js",
    "start": "node server-build/index.js",
    "clean": "del .cache build server-build public/build app/components/icons",
    "lint": "eslint --cache --ext js,jsx,ts,tsx,mdx ."
  },
  "repository": "mcansh/remix-feature-flags",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh/)",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "0.2.0",
    "@lukeed/uuid": "2.0.0",
    "@prisma/client": "2.15.0",
    "@remix-run/cli": "0.9.1",
    "@remix-run/core": "0.9.1",
    "@remix-run/data": "0.9.1",
    "@remix-run/express": "0.9.1",
    "@remix-run/react": "0.9.1",
    "change-case": "4.1.2",
    "clsx": "1.1.1",
    "connect-redis": "5.0.0",
    "date-fns": "2.16.1",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "framer-motion": "3.2.2-rc.1",
    "heroicons": "0.4.2",
    "ioredis": "4.19.4",
    "ms": "2.1.3",
    "postmark": "2.7.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "secure-password": "4.0.0",
    "slugify": "1.4.6",
    "type-fest": "0.20.2"
  },
  "devDependencies": {
    "@mcansh/eslint-config": "3.0.1",
    "@prisma/cli": "2.15.0",
    "@svgr/core": "5.5.0",
