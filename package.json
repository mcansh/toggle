    "eslint": "7.18.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "nodemon": "2.0.7",
    "pm2": "4.5.1",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
